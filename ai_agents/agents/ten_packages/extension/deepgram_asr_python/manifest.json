{
  "type": "extension",
  "name": "deepgram_asr_python",
  "version": "0.1.0",
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.10"
    },
    {
      "path": "/tmp/ten_ai_base"
    }
  ],
  "api": {
    "property": {
      "properties": {
        "api_key": {
          "type": "string"
        },
        "model": {
          "type": "string"
        },
        "language": {
          "type": "string"
        },
        "sample_rate": {
          "type": "int64"
        }
      }
    },
    "cmd_in": [
      {
        "name": "on_user_joined",
        "property": {
          "properties": {
            "user_id": {
              "type": "string"
            }
          }
        }
      },
      {
        "name": "on_user_left",
        "property": {
          "properties": {
            "user_id": {
              "type": "string"
            }
          }
        }
      },
      {
        "name": "on_connection_failure",
        "property": {
          "properties": {
            "error": {
              "type": "string"
            }
          }
        }
      }
    ],
    "data_out": [
      {
        "name": "text_data",
        "property": {
          "properties": {
            "time": {
              "type": "int64"
            },
            "duration_ms": {
              "type": "int64"
            },
            "language": {
              "type": "string"
            },
            "text": {
              "type": "string"
            },
            "is_final": {
              "type": "bool"
            },
            "stream_id": {
              "type": "uint32"
            },
            "end_of_segment": {
              "type": "bool"
            }
          }
        }
      }
    ],
    "audio_frame_in": [
      {
        "name": "pcm_frame",
        "property": {
          "properties": {}
        }
      }
    ]
  }
}